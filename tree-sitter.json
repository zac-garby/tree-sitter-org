{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "org",
      "camelcase": "Org",
      "title": "Org mode",
      "scope": "source.org",
      "file-types": [
        "org"
      ],
      "injection-regex": "^org$",
      "class-name": "TreeSitterOrg"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Org grammar for tree-sitter",
    "authors": [
      {
        "name": "Zac Garby",
        "email": "me@zacgarby.co.uk"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-org"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}